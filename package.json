  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo-js/is-any-array/issues"
  },
  "homepage": "https://github.com/cheminfo-js/is-any-array#readme",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "@zakodium/tsconfig": "^1.0.2",
    "eslint": "^9.39.2",
    "eslint-config-cheminfo-typescript": "^21.1.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
